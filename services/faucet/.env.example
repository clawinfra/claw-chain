# ClawChain node WebSocket RPC URL
RPC_URL=ws://localhost:9944

# Faucet account seed (sr25519). Can be mnemonic or dev account URI like //Alice
FAUCET_SEED=//Alice

# GitHub OAuth App credentials (create at https://github.com/settings/developers)
# Callback URL: https://faucet.clawchain.win/auth/github/callback
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# Secret for express-session cookie signing â€” CHANGE THIS in production!
SESSION_SECRET=change-me-in-production

# HTTP port to listen on
PORT=3000

# SQLite database file path
DB_PATH=./faucet.db
